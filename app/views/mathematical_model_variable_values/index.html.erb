<table>
  <tr>
    <th>Environmental parameter</th>
    <th>Decision parameter</th>
    <th>Mathematical model variable</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mathematical_model_variable_values.each do |mathematical_model_variable_value| %>
  <tr>
    <td><%= mathematical_model_variable_value.environmental_parameter_id %></td>
    <td><%= mathematical_model_variable_value.decision_parameter_id %></td>
