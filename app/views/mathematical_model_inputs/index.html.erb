<table>
  <tr>
    <th>Mathematical model</th>
    <th>Environmental parameter</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mathematical_model_inputs.each do |mathematical_model_input| %>
  <tr>
    <td><%= mathematical_model_input.mathematical_model_id %></td>
    <td><%= mathematical_model_input.environmental_parameter_id %></td>
    <td><%= link_to 'Show', mathematical_model_input %></td>
